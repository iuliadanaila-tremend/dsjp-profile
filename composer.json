        "drupal/core": "^9.3",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/ctools": "^3.6",
        "drupal/default_content": "^2.0@alpha",
        "drupal/dropsolid_purge": "^1.0@beta",
        "drupal/editor_advanced_link": "^1.9",
        "drupal/embed": "^1.4",
        "drupal/extlink": "^1.6",
        "drupal/facets": "^1.7",
        "drupal/field_group": "^3.1",
        "drupal/fixed_block_content": "^1.1",
        "drupal/flag": "^4.0@beta",
        "drupal/focal_point": "^1.5",
        "drupal/geocoder": "^3.15",
        "drupal/geofield": "^1.22",
        "drupal/geolocation": "^3.2",
        "drupal/ginvite": "^1.1",
        "drupal/grequest": "^1.0@RC",
        "drupal/group": "^1.3",
        "drupal/group_permissions": "^1.0@alpha",
        "drupal/image_widget_crop": "^2.3",
        "drupal/install_profile_generator": "^3.0@alpha",
        "drupal/json_field": "^1.0@RC",
        "drupal/jsonapi_extras": "^3.19",
        "drupal/legal": "^2.0",
        "drupal/linkit": "^5.0@beta",
        "drupal/matomo": "^1.11",
        "drupal/maxlength": "^1.0@RC",
        "drupal/mentions": "^1.0@alpha",
        "drupal/menu_item_extras": "^2.13",
        "drupal/message_digest": "^1.2",
        "drupal/message_subscribe": "^1.1",
        "drupal/metatag": "^1.16",
        "drupal/migrate_file": "^2.0",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/migrate_tools": "^5.0",
        "drupal/override_node_options": "^2.6",
        "drupal/paragraph_view_mode": "^2.0",
        "drupal/pathauto": "^1.8",
        "drupal/private_message": "^2.0@beta",
        "drupal/purge": "^3.0",
        "drupal/queue_ui": "^2.2",
        "drupal/recaptcha": "^3.0",
        "drupal/recaptcha_v3": "^1.4",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/schema_metatag": "^2.2",
        "drupal/search_api": "^1.19",
        "drupal/search_api_solr": "^4.1",
        "drupal/select2": "^1.13",
        "drupal/shs": "^1.0@alpha",
        "drupal/simplenews": "^3.0",
        "drupal/sitemap": "^2.0",
        "drupal/slick_entityreference": "^2.0",
        "drupal/slick_paragraphs": "^2.1",
        "drupal/slick_views": "^2.3",
        "drupal/swiftmailer": "^2.0",
        "drupal/tmgmt": "^1.12",
        "drupal/typed_link": "^2.0",
        "drupal/tzfield": "^1.3",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/user_online_status": "^1.0",
        "drupal/varnish_purge": "^2.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bulk_operations": "^3.12",
        "drupal/views_custom_cache_tag": "^1.2",
        "drupal/views_data_export": "^1.0",
        "drupal/views_infinite_scroll": "^1.8",
        "drupal/webform": "^6.1",
        "drupal/webform_views": "^5.0@alpha",
        "drupal/xmlsitemap": "^1.0",
        "drupal/yearonly": "^9.0",
        "drush/drush": "^10.0",
        "easyrdf/easyrdf": "1.0.0 as 0.9.1",
        "harvesthq/chosen": "^1.8",
        "npm-asset/moment": "^2.29",
        "npm-asset/moment-timezone": "^0.5.33",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "openeuropa/composer-artifacts": "^1.0@alpha",
        "openeuropa/oe_authentication": "1.4.0",
        "openeuropa/oe_corporate_blocks": "^4.0",
        "openeuropa/oe_media": "^1.12",
        "openeuropa/oe_multilingual": "^1.5",
        "openeuropa/oe_profile": "^0.5",
        "openeuropa/oe_theme": "^3",
        "openeuropa/oe_webtools": "^1.14",
        "openeuropa/rdf_skos": "~1.0.0-alpha1",
        "openeuropa/webtools-geocoding-provider": "^0.1.2",
        "pear/archive_tar": "^1.4.14",
        "webflo/drupal-finder": "^1.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3",
        "drupal/devel": "^4.0",
        "drupal/drupal-extension": "~4.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/purge_control": "^1.0",
        "drupal/stage_file_proxy": "^1.1",
        "ec-europa/toolkit": "^8.6.10",
        "friendsoftwig/twigcs": ">=4"
    },
    "scripts": {
      "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "repositories": [
      {
          "type": "composer",
          "url": "https://packages.drupal.org/8"
      },
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      },
      {
        "type": "package",
        "package": {
          "name": "harvesthq/chosen",
          "version": "1.8.7",
          "type": "drupal-library",
          "source": {
            "url": "https://github.com/harvesthq/chosen-package.git",
            "type": "git",
            "reference": "v1.8.7"
          }
        }
      },
     {
        "type": "package",
        "package": {
          "name": "bower-asset/timepicker",
          "version": "1.11.14",
          "type": "drupal-library",
          "source": {
            "url": "https://github.com/jonthornton/jquery-timepicker.git",
            "type": "git",
            "reference": "1.11.14"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "kenwheeler/slick",
          "version": "1.8.1",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/kenwheeler/slick/archive/v1.8.1.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "alexsobolenko/flag-icons",
          "version": "1.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/alexsobolenko/flag-icons/archive/master.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "ckeditor/colorbutton",
          "version": "4.16.1",
          "type": "drupal-library",
          "dist": {
            "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.16.1.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "name": "ckeditor/panelbutton",
          "version": "4.16.1",
          "type": "drupal-library",
          "dist": {
            "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.16.1.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "git",
        "url": "https://git.drupalcode.org/project/sparql_entity_storage.git"
      },
      {
        "type": "package",
        "package": {
          "name": "ckeditor/editorplaceholder",
          "version": "4.16.2",
          "type": "drupal-library",
          "dist": {
            "url": "https://download.ckeditor.com/editorplaceholder/releases/editorplaceholder_4.16.2.zip",
            "type": "zip"
          }
        }
      },
      {
        "type": "vcs",
        "url": "https://github.com/openeuropa/oe_dashboard_agent"
      }
  ],
      "extra": {
        "artifacts": {
            "openeuropa/oe_theme": {
                "dist": {
                  "url": "https://github.com/{name}/releases/download/{pretty-version}/{project-name}-{pretty-version}.zip",
                  "type": "zip"
                }
            }
        },
        "composer-exit-on-patch-failure": false,
        "enable-patching": true,
        "installer-types": [
          "bower-asset",
          "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset",
                "harvesthq/chosen",
                "bower-asset/timepicker",
                "kenwheeler/slick",
                "dinbror/blazy",
                "alexsobolenko/flag-icons"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-scaffold": {
          "locations": {
            "web-root": "./web"
          }
        },
        "patches": {
            "drupal/core": {
              "MethodNotAllowedException thrown on POST requests if user has set admin language in their profile": "https://www.drupal.org/files/issues/2019-04-15/route-method-exception-path-fix-3048288.patch",
              "It is possible to overflow the number of items allowed in Media Library": "https://www.drupal.org/files/issues/2020-11-16/3059955-107.patch",
              "States API doesn't work with multiple select fields": "https://www.drupal.org/files/issues/2019-05-26/states-multiselect-1149078-109.patch",
              "Notice: Undefined index: #type in drupal_process_states": "https://www.drupal.org/files/issues/2021-05-12/2700667-133a.patch",
              "Notice: Undefined index: value in Drupal\\views\\Plugin\\views\\filter\\NumericFilter->acceptExposedInput()": "https://www.drupal.org/files/issues/2021-05-07/2825860-exposed-filter-notice-56.patch",
              "Translations of nodes with comment settings field set to non-translatable returns EntityUntranslatableFieldsConstraint": "https://www.drupal.org/files/issues/2020-07-11/3015675-19.patch",
              "Add a $properties argument to FieldItemList::equals()": "https://www.drupal.org/files/issues/2019-02-11/ignore_width_height_on_untranslatable_field_comparision_2941092_8.patch",
              "Enabling aggregation breaks views with \"Moderation state\" fields": "https://www.drupal.org/files/issues/2020-08-07/3135409-6.patch",
              "Draft translations should be based on the latest draft of the source language, not the published version": "https://www.drupal.org/files/issues/2021-11-01/3007233-draft_translations_based_on_draft-16.patch",
              "Display Bug when using #states (Forms API) with Ajax Request": "https://www.drupal.org/files/issues/2021-11-05/1091852-150.patch",
              "Allow end date to be optional": "https://www.drupal.org/files/issues/2022-06-16/drupal-datetime_range-allow_end_date_optional-2794481-139.patch"
            },
            "drupal/message_ui": {
              "Deleted and replaced configuration entity error": "https://www.drupal.org/files/issues/2018-12-21/issue-3021923-deleted-replaced-config-error-1.patch"
            },
            "drupal/message_subscribe": {
              "Issue #3101137: Fix endless loop": "https://www.drupal.org/files/issues/2019-12-15/3101137-fix-endless-loop.patch"
            },
            "openeuropa/oe_theme": {
              "Infinite loop - Maximum call stack size exceeded.": "./patches/oe_theme_ecl_datepicker_infinite_loop.patch",
              "Pager breaks on ajax requests": "./patches/oe_theme_pager_crash_ajax_pages.patch"
            },
            "drupal/group": {
              "Error when trying to set available content.": "https://www.drupal.org/files/issues/2018-08-08/group-error_set_available_content-2991390-2.patch",
              "Get a token of a node's parent group to create a pathauto pattern": "https://www.drupal.org/files/issues/2020-11-17/group-gnode_tokens-2774827-74.patch",
              "Joining a group dumps the user on their account page instead of returning them to the group": "https://www.drupal.org/files/issues/2021-03-18/joined_user_redirection-2980034-18.patch"
            },
            "drupal/legal": {
              "Separate extra fields from the main additional checkboxes and add update trigger": "./patches/legal_extra_checkboxes.patch",
              "Preserve CAS module returnto destiantion": "./patches/legal_cas_returnto.patch"
            },
            "drupal/views_data_export": {
              "Hide empty columns for CSV export": "https://www.drupal.org/files/issues/2020-09-04/views_data_export-hide-empty-columns-csv-3169049-2.patch",
              "Feed icon is displaying irrespective of view access parameter in attached page": "https://www.drupal.org/files/issues/2020-09-25/feed_icon_displayed_irrespective_access-3135831-3.patch"
            },
            "drupal/group_permissions": {
              "Fix issue when group is missing but there are permissions set.": "./patches/group_permissions_deleted_group.patch"
            },
            "drupal/views_infinite_scroll": {
              "Views infinite scroll VBO workaround": "./patches/views_infinite_scroll_vbo.patch",
              "Any way to Automatically Load Content when scrolling within a DIV?": "https://www.drupal.org/files/issues/2021-02-09/support_for_overflow-_scrolls--2894307-21.patch"
            },
            "drupal/field_group": {
              "Tabs with invalid input are not focused": "https://www.drupal.org/files/issues/2021-04-18/2894351-23.patch"
            },
            "drupal/private_message": {
              "Fix view mode class wrapper": "./patches/private_message_view_mode.patch"
            },
            "drupal/default_content": {
              "TypeError: Argument 1 passed to default_content": "https://www.drupal.org/files/issues/2021-05-03/default_content-3202244-null_dependency-5.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SetupWizard.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Site\\Tests\\": "./tests/"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "process-timeout": 0,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "phpro/grumphp": true,
            "openeuropa/composer-artifacts": true,
            "oomphinc/composer-installers-extender": true,
            "drupal-composer/drupal-scaffold": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
