{
    "name": "digit/dsjp",
    "description": "A one-stop-shop for digital skills and jobs in the EU.",
    "type": "profile",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "alexsobolenko/flag-icons": "^1.0",
        "bower-asset/timepicker": "^1.11",
        "composer/installers": "^1.5",
        "cweagans/composer-patches": "^1.0",
        "drupal/address": "^1.9",
        "drupal/admin_audit_trail": "^1.0@beta",
        "drupal/admin_toolbar": "^2.5",
        "drupal/ajax_comments": "^1.0@beta",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/chosen": "^3.0",
        "drupal/ckeditor_font": "^1.2",
        "drupal/colorbutton": "^1.2",
        "drupal/comments_order": "^2.0",
        "drupal/config_filter": "^2.2",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^2.0@beta",
