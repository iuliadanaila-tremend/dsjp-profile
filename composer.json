    "require-dev": {
        "drupal/core-dev": "^9.4",
        "drupal/devel": "^5",
        "drupal/drupal-extension": "~4.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/purge_control": "^1.0",
        "drupal/stage_file_proxy": "^2",
        "drupal/upgrade_status": "^4.0",
        "ec-europa/toolkit": "^9",
        "friendsoftwig/twigcs": ">=4"
    },
     "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/run toolkit:hooks-enable"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || ./vendor/bin/run toolkit:hooks-enable"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/harvesthq/chosen-package.git",
                    "type": "git",
                    "reference": "v1.8.7"
                }
            }
        },
        "jquery-timepicker": {
            "type": "package",
            "package": {
                "name": "bower-asset/timepicker",
                "version": "1.11.14",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/jonthornton/jquery-timepicker.git",
                    "type": "git",
                    "reference": "1.11.14"
                }
            }
        },
        "slick": {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/v1.8.1.zip",
                    "type": "zip"
                }
            }
        },
        "blazy": {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
            }
        },
        "flag-icons": {
            "type": "package",
            "package": {
                "name": "alexsobolenko/flag-icons",
                "version": "1.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/alexsobolenko/flag-icons/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        "jquery-ui-touch-punch": {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        },
        "colorbutton": {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "version": "4.16.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.16.1.zip",
                    "type": "zip"
                }
            }
        },
        "panelbutton": {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "version": "4.16.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.16.1.zip",
                    "type": "zip"
                }
            }
        },
        "sparql_entity_storage": {
            "type": "git",
            "url": "https://git.drupalcode.org/project/sparql_entity_storage.git"
        },
        "editorplaceholder": {
            "type": "package",
            "package": {
                "name": "ckeditor/editorplaceholder",
                "version": "4.16.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/editorplaceholder/releases/editorplaceholder_4.16.2.zip",
                    "type": "zip"
                }
            }
        },
        "oe_dashboard_agent": {
            "type": "vcs",
            "url": "https://github.com/openeuropa/oe_dashboard_agent"
        }
    },
    "extra": {
        "artifacts": {
            "openeuropa/oe_theme": {
                "dist": {
                    "url": "https://github.com/{name}/releases/download/{pretty-version}/{project-name}-{pretty-version}.zip",
                    "type": "zip"
                }
            }
        },
        "composer-exit-on-patch-failure": false,
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset",
                "harvesthq/chosen",
                "bower-asset/timepicker",
                "kenwheeler/slick",
                "dinbror/blazy",
                "alexsobolenko/flag-icons"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "patches": {
            "drupal/core": {
                "MethodNotAllowedException thrown on POST requests if user has set admin language in their profile": "https://www.drupal.org/files/issues/2019-04-15/route-method-exception-path-fix-3048288.patch",
                "It is possible to overflow the number of items allowed in Media Library": "https://www.drupal.org/files/issues/2020-11-16/3059955-107.patch",
                "States API doesn't work with multiple select fields": "https://www.drupal.org/files/issues/2022-12-23/states-multiselect-1149078-142.patch",
                "Notice: Undefined index: #type in drupal_process_states": "https://www.drupal.org/files/issues/2021-05-12/2700667-133a.patch",
                "Notice: Undefined index: value in Drupal\\views\\Plugin\\views\\filter\\NumericFilter->acceptExposedInput()": "https://www.drupal.org/files/issues/2021-05-07/2825860-exposed-filter-notice-56.patch",
                "Translations of nodes with comment settings field set to non-translatable returns EntityUntranslatableFieldsConstraint": "https://www.drupal.org/files/issues/2020-07-11/3015675-19.patch",
                "Add a $properties argument to FieldItemList::equals()": "https://www.drupal.org/files/issues/2019-02-11/ignore_width_height_on_untranslatable_field_comparision_2941092_8.patch",
                "Enabling aggregation breaks views with \"Moderation state\" fields": "https://www.drupal.org/files/issues/2020-08-07/3135409-6.patch",
                "Display Bug when using #states (Forms API) with Ajax Request": "https://www.drupal.org/files/issues/2022-12-16/1091852-163.patch",
                "Allowed start date and end date to be optional": "https://www.drupal.org/files/issues/2020-09-08/3133982-2.patch",
                "ManyToOneHelper ignores group configuration for some cases": "https://www.drupal.org/files/issues/2021-08-18/2559961-105.patch",
                "Call to a member function transformDimensions() on null in template_preprocess": "https://www.drupal.org/files/issues/2022-07-28/2957368-40.patch",
                "PHP8.1 deprecation: str_replace()": "https://www.drupal.org/files/issues/2022-07-07/3294680-2.patch",
                "PHP8.1 deprecation": "https://www.drupal.org/files/issues/2022-10-03/utility.patch",
                "Recoverable fatal error: Argument 2 passed": "https://www.drupal.org/files/issues/2020-10-20/relationship_handler_bugfix-2806015-10.patch"
            },
            "drupal/message_ui": {
                "Deleted and replaced configuration entity error": "https://www.drupal.org/files/issues/2018-12-21/issue-3021923-deleted-replaced-config-error-1.patch"
            },
            "drupal/message_subscribe": {
                "Issue #3101137: Fix endless loop": "https://www.drupal.org/files/issues/2019-12-15/3101137-fix-endless-loop.patch"
            },
            "drupal/group": {
                "Error when trying to set available content.": "https://www.drupal.org/files/issues/2018-08-08/group-error_set_available_content-2991390-2.patch",
                "Get a token of a node's parent group to create a pathauto pattern": "https://www.drupal.org/files/issues/2020-11-17/group-gnode_tokens-2774827-74.patch",
                "Joining a group dumps the user on their account page instead of returning them to the group": "https://www.drupal.org/files/issues/2021-03-18/joined_user_redirection-2980034-18.patch",
                "Fix missing revisions tab" : "https://www.drupal.org/files/issues/2022-08-10/group-2876696-21-and-3256998-51.patch"
            },
            "drupal/views_data_export": {
                "Hide empty columns for CSV export": "https://www.drupal.org/files/issues/2020-09-04/views_data_export-hide-empty-columns-csv-3169049-2.patch",
                "Feed icon is displaying irrespective of view access parameter in attached page": "https://www.drupal.org/files/issues/2020-09-25/feed_icon_displayed_irrespective_access-3135831-3.patch"
            },
            "drupal/webform_views": {
                "Webform sumission view filter for multivalues": "https://www.drupal.org/files/issues/2022-06-21/webform_view_filter_multivalued_field_3068861_13.patch"
            },
            "drupal/views_infinite_scroll": {
                "Any way to Automatically Load Content when scrolling within a DIV?": "https://www.drupal.org/files/issues/2021-02-09/support_for_overflow-_scrolls--2894307-21.patch"
            },
            "drupal/field_group": {
                "Tabs with invalid input are not focused": "https://www.drupal.org/files/issues/2021-04-18/2894351-23.patch"
            },
            "drupal/tmgmt": {
                "Hook to allow modules to alter translation entity being created": "https://www.drupal.org/files/issues/2022-07-19/hook_implementation-3298245-2.patch"
            },
            "drupal/search_api_solr": {
                "Warning: Undefined array key": "https://www.drupal.org/files/issues/2022-10-24/search_api_solr-empty-specific-languages-3314711-21.patch"
            },
            "openeuropa/oe_webtools": {
                "Fix PHP 8.1 deprecation but keep PHP 7.4 compatibility": "https://patch-diff.githubusercontent.com/raw/openeuropa/oe_webtools/pull/182.patch"
            },
            "drupal/twig_field_value": {
                "Upgrade 2.0.1 too few arguments to function": "https://www.drupal.org/files/issues/2022-10-19/3315171-13.patch"
            },
            "easyrdf/easyrdf": {
                "Fix 8.1 deprecation warnings (https://github.com/easyrdf/easyrdf/pull/398)": "https://patch-diff.githubusercontent.com/raw/easyrdf/easyrdf/pull/398.patch"
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SetupWizard.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEuropa\\Site\\Tests\\": "./tests/"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "process-timeout": 0,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "phpro/grumphp": true,
            "openeuropa/composer-artifacts": true,
            "oomphinc/composer-installers-extender": true,
            "drupal-composer/drupal-scaffold": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true
        }
    }
}
