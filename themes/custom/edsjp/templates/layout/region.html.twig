{%
set classes = [
'region',
'region-' ~ region|clean_class,
]
%}

{% if content|render %}
    <div{{ attributes.addClass(['region', 'region-content']) }}>
      {% if dashboard_info %}
        <div class="user-dashboard-intro">
          {{ dashboard_info }}
        </div>
      {% endif %}
        {{ content }}
    </div>
{% endif %}
